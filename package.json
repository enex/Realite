{
  "name": "realite",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "bun run server.ts",
    "dev": "concurrently \"bun run server\" \"expo start\"",
    "lint": "expo lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run db/migrate.ts",
    "db:seed": "bun run db/seed.ts",
    "db:drop": "drizzle-kit drop",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "build": "expo export --platform web",
    "build:web": "expo export --platform web",
    "build:android": "export JAVA_HOME=/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home && NODE_ENV=production dotenvx run -f .env.local -- eas build --platform android --local",
    "build:ios": "NODE_ENV=production dotenvx run -f .env.local -- eas build --platform ios --local",
    "submit:android": "eas submit --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "testflight": "npx testflight",
    "screenshots": "bun run scripts/generate-screenshots.ts",
    "screenshots:capture": "bun run scripts/capture-web-screenshots.ts",
    "screenshots:all": "bun run screenshots:capture",
    "upgrade": "expo install --fix && bunx expo-doctor@latest && echo 'https://docs.expo.dev/workflow/upgrading-expo-sdk-walkthrough/'"
  },
  "dependencies": {
